package angryProfessor;

import java.util.Scanner;

public class Solution {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int T = sc.nextInt();
		while (T-- > 0) {
			int N = sc.nextInt();
			int K = sc.nextInt();
			int count = 0;
			for (int i = 0; i < N; i++) {
				if (sc.nextInt() <= 0) {
					count++;
				}
			}
			if (count >= K) {
				System.out.println("NO");
			} else {
				System.out.println("YES");
			}
		}
		sc.close();
	}
}
