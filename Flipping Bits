package flippingBits;

import java.util.*;

public class Solution {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		long T = sc.nextInt();
		long num;
		long temp = (1L << 32) - 1;
		for (int i = 0; i < T; i++) {
			num = sc.nextLong();
			System.out.println(num ^ temp);
		}
		sc.close();
	}
}
