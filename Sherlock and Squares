package sherlockAndSquares;

import java.util.*;

public class Solution {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int num = sc.nextInt();
		int firstNum, secondNum;
		for (int i = 0; i < num; i++) {
			firstNum = sc.nextInt();
			secondNum = sc.nextInt();
			System.out.println((int) Math.floor(Math.sqrt(secondNum))
					- (int) Math.ceil(Math.sqrt(firstNum)) + 1);
		}
		sc.close();
	}
}
